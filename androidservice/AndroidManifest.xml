<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidservice"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />
<uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" ></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon2"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        >
        <service android:name="com.example.androidservice.ConnectService" >
            <intent-filter>
                <action android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </service>

        <activity
            android:name="com.example.androidservice.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.androidservice.RegisterActivity" >
        </activity>
        <activity android:name="com.example.androidservice.NaviActivity" >
        </activity>
        <activity android:name="com.example.androidservice.PersonInfoActivity" >
        </activity>
        <activity android:name="com.example.androidservice.UpdatePasswordActivity" >
        </activity>
        <activity android:name="com.example.androidservice.PayActivity" >
        </activity>
        <activity android:name="com.example.androidservice.SettingActivity" >
        </activity>
        <activity android:name="com.example.androidservice.PasswordActivity" >
        </activity>
        <activity android:name="com.example.androidservice.CheckPasswordActivity" >
        </activity>
        <activity android:name="com.example.androidservice.BindingActivity" >
        </activity>
        <activity android:name="com.example.androidservice.CardActivity" >
        </activity>
        <activity android:name="com.example.androidservice.CardPayActivity" >
        </activity>
        <activity android:name="com.example.androidservice.CheckEmailActivity" >
        </activity>
    </application>

</manifest>